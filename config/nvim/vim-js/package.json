{
  "name": "vim-js",
  "version": "1.0.0",
  "description": "",
  "author": "David <david.susskind@walkme.com>",
  "license": "MIT",
  "main": "lib/index.js",
  "keywords": [
    "coc.nvim"
  ],
  "engines": {
    "coc": "^0.0.77"
  },
  "scripts": {
    "lint": "eslint src --ext ts",
    "clean": "rimraf lib",
    "watch": "webpack --watch",
    "build": "webpack",
    "prepare": "npm-run-all clean build"
  },
  "prettier": {
    "arrowParens": "avoid",
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 120,
    "semi": false
  },
  "devDependencies": {
    "@types/node": "^14.14.10",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "coc.nvim": "^0.0.79",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.0",
    "rimraf": "^3.0.2",
    "ts-loader": "^8.0.11",
    "typescript": "^4.1.2",
    "webpack": "^5.9.0",
    "webpack-cli": "^4.2.0"
  },
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "tmp configuration",
      "properties": {
        "tmp.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable tmp extension"
        }
      }
    },
    "commands": [
      {
        "command": "tmp.Command",
        "title": "tmp command title"
      }
    ]
  },
  "dependencies": {
    "debounce": "^1.2.0",
    "fs-extra": "^9.0.1"
  }
}
